<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
	<changeSet author="sheshank" id="creating table">
		<createTable tableName="users_test">
			<column name="username" type="varchar(30)">
				<constraints primaryKey="true" nullable="false"/>
			</column>
			<column name="password" type="varchar(20)"></column>
			<column name="firstname" type="char(30)"></column>
			<column name="lastname" type="char(30)"></column>
			<column name="address" type="varchar(30)"></column>
			<column name="phone" type="numeric(15)"></column>
			<column name="test_result" type="char(20)"></column>
		</createTable>
	</changeSet>
</databaseChangeLog>